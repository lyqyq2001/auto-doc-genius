{
  "name": "auto-doc-genius",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "docxtemplater": "^3.67.6",
    "element-plus": "^2.6.3",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "pizzip": "^3.2.0",
    "temp": "^0.9.4",
    "vue": "^3.5.24",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "terser": "^5.44.1",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0"
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.example.myapp",
    "productName": "AutoDoc Genius",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "compression": "maximum",
    "asar": true,
    "asarUnpack": [
      "node_modules/docxtemplater/**/*",
      "node_modules/pizzip/**/*",
      "node_modules/xlsx/**/*",
      "node_modules/temp/**/*",
      "node_modules/rimraf/**/*",
      "electron/**/*"
    ],
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "electron/**/*",
      "node_modules/temp/**/*",
      "node_modules/rimraf/**/*",
      "node_modules/glob/**/*",
      "node_modules/minimatch/**/*",
      "node_modules/docxtemplater/**/*",
      "node_modules/pizzip/**/*",
      "node_modules/xlsx/**/*",
      "!src/**/*",
      "!*.md",
      "!.gitignore",
      "!.vscode/**/*",
      "!package-lock.json"
    ],
    "extraResources": [],
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:3000/"
      }
    ]
  }
}
